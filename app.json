            },
            "edgeToEdgeEnabled": true,
            "jsEngine": "jsc",
            "package": "com.electrikfire7.expenseTracker"
        },
        "web": {
            "favicon": "./assets/rupeesexpensetracker.png"
        },
        "plugins": ["expo-sqlite"],
        "extra": {
            "eas": {
                "projectId": "3e0ac517-6647-4f5d-9ef0-0702e5fbf442"
            }
        }
