{
    "expo": {
        "name": "expenseTracker",
        "slug": "expenseTracker",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/rupeesexpensetracker.png",
        "userInterfaceStyle": "light",
        "newArchEnabled": true,
        "jsEngine": "jsc",
        "splash": {
            "image": "./assets/rupeesexpensetracker.png",
